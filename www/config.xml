<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0"  id="com.coladaservices.scanEventTickets"
        version="1.0.5">
    <name>scanEventTickets</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <plugin name="cordova-plugin-whitelist" version="1"/>
   
    <content src="index.html"/>
    <access origin="*"/>
    <preference name="fullscreen" value="false"/>
    <preference name="DisallowOverscroll" value="true" />
    <!--<preference name="phonegap-version" value="3.5.0" />-->

    <!--<gap:plugin name="org.apache.cordova.dialogs"/>-->
    <plugin name="net.yoik.cordova.plugins.screenorientation" version="1.3.1"/>
    <plugin spec="https://github.com/apache/cordova-plugin-network-information.git" source="git" />
    <plugin spec="https://github.com/apache/cordova-plugin-statusbar.git" source="git" />
    <plugin spec="https://github.com/apache/cordova-plugin-device.git" source="git" />
    <plugin spec="https://github.com/phonegap/phonegap-plugin-barcodescanner.git" source="git" />
    <plugin spec="https://github.com/apache/cordova-plugin-camera.git" source="git" />

    <!--<gap:plugin name="org.apache.cordova.network-information" />-->
    <!--<gap:plugin name="org.apache.cordova.statusbar" version="0.1.4"/>-->
    <!--<gap:plugin name="org.apache.cordova.device"/>-->
    <!--<gap:plugin name="com.phonegap.plugins.barcodescanner"/>-->
    <!--<gap:plugin name="org.apache.cordova.camera" version="0.2.3" />-->

    <feature name="http://api.phonegap.com/1.0/network" />
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" onload="true" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="Notification"/>
    </feature>
    <feature name="http://api.phonegap.com/1.0/file"/>
    <feature name="http://api.phonegap.com/1.0/camera"/>
    <icon src="/res/icon/icon.png"/>
    <splash src="res/screen/splash.png"/>

    <!--<gap:plugin name="com.ionic.keyboard" version="1.0.3" />-->
    <plugin spec="https://github.com/driftyco/ionic-plugin-keyboard.git" source="git" />
    <gap:splash src="res/screen/splash.png"/>
    <!-- -->
    <icon gap:platform="android" height="96" src="/res/icon/android/icon-96-xhdpi.png" width="96"/>
    <icon gap:platform="android" height="72" src="/res/icon/android/icon-72-hdpi.png" width="72"/>
    <icon gap:platform="android" height="48" src="/res/icon/android/icon-48-mdpi.png" width="48"/>
    <icon gap:platform="android" height="36" src="/res/icon/android/icon-36-ldpi.png" width="36"/>
    
    <gap:splash gap:platform="android" height="800" src="/res/screen/android/screen-hdpi-portrait.png" width="480"/>
    <gap:splash gap:platform="android" height="320" src="/res/screen/android/screen-hdpi-portrait.png" width="200"/>
    <gap:splash gap:platform="android" height="480" src="/res/screen/android/screen-hdpi-portrait.png" width="320"/>
